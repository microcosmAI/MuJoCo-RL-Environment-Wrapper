    <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="hip_3" gear="150"/>
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="ankle_3" gear="150"/>
  </actuator>

  <sensor>
        <rangefinder name="box1" site="box_range" cutoff="20"/>
        <framexaxis name="yAxis" objtype="site" objname="box_range"/>
        <rangefinder name="touch_top1" site="box_sensor_top" cutoff="20"/>
    </sensor>
</mujoco>
