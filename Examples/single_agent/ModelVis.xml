    </body>
    <body name="torso" pos="-6 -2 1" euler="0 0 0">
      <camera name="track" mode="trackcom" pos="0 -3 0.3" xyaxes="1 0 0 0 0 1"/>
      <site name="box_range" type="box" pos="0.2 0 0" size=".05 .05 .05" rgba="0 .9 0 1" euler="0 90 0"/>
